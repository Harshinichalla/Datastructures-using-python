    "x=np.array([1,2,3,4,5])\n",
    "def two(x):\n",
    "    length=len(x)\n",
    "    add_all=np.sum(x)\n",
    "    average=add_all/length\n",
    "    return add_all,average\n",
    "result=two(x)\n",
    "print(result)"
